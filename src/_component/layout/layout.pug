doctype html
html
  block config
  head
    title=title
    meta(
        name="description", 
        content=description)
    meta(
        name="robots", 
        content="noindex"
        )
    meta(
        name="viewport", 
        content="width=device-width", 
        initial-scale=1.0, 
        maximum-scale=1.0, 
        user-scalable=no
        )
    link(
        rel="stylesheet", href=`${embedPath}` embed)
    link(
        rel="canonical", 
        href="https://robertoblake.com/"
        )
  body
    include ../header/header
    block content
    include ../footer/footer
